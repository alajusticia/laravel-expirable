{
    "name": "alajusticia/laravel-expirable",
    "description": "Make Eloquent models expirable",
    "license": "MIT",
    "authors": [
        {
            "name": "Anthony Lajusticia",
            "email": "anthonylajusticia@gmail.com",
            "homepage": "https://anthonylajusticia.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0",
        "nesbot/carbon": "^2.67|^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "ALajusticia\\Expirable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ALajusticia\\Expirable\\Tests\\": "tests",
            "ALajusticia\\Expirable\\Tests\\Database\\Factories\\": "tests/database/factories"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ALajusticia\\Expirable\\ExpirableServiceProvider"
            ]
        }
    }
}
